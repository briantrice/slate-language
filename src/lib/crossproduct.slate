collections define: #CrossProduct &slots: {#rows -> #{}. #columns -> #{}}.

rows@(Collection traits) <*> columns@(Collection traits)
[
  CrossProduct new `>> [rows := rows. columns := columns. ]
].

cross@(CrossProduct traits) do: block
[
  cross rows do:
    [| :row |
     cross columns do:
       [| :column |
    block apply*, row, column]]
].
